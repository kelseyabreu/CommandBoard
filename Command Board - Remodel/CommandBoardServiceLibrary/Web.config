<configuration>
  <system.serviceModel>
    <client>
      <endpoint binding="netTcpBinding" bindingConfiguration="" contract="CommandBoardServiceLibrary.ICommandBoardService"
        name="EndPointTCP" />
    </client>
    <services>
      <service behaviorConfiguration="CommandBoardServiceLibrary.CommandBoardServiceBehavior"
        name="CommandBoardServiceLibrary.CommandBoardService">
        <endpoint address="" binding="netTcpBinding" bindingConfiguration=""
          contract="CommandBoardServiceLibrary.ICommandBoardService">
          <identity>
            <dns value="localhost" />
          </identity>
        </endpoint>
        <endpoint address="mex" binding="mexTcpBinding" bindingConfiguration=""
          contract="IMetadataExchange" />
        <host>
          <baseAddresses>
            <add baseAddress="net.tcp://localhost:9000/commandboard" />
          </baseAddresses>
        </host>
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior name="CommandBoardServiceLibrary.CommandBoardServiceBehavior">
          <serviceMetadata httpGetEnabled="false" />
          <serviceDebug includeExceptionDetailInFaults="false" />
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>
</configuration>